{
  "name": "node-sequilize",
  "version": "0.0.1",
  "description": "Boilerplate application for Node.js",
  "main": "dist/index.js",
  "types": "dist/@types",
  "scripts": {
    "start:dev": "NODE_ENV=dev node dist/index.js",
    "start:qa": "NODE_ENV=qa node dist/index.js",
    "start:uat": "NODE_ENV=uat node dist/index.js",
    "start:prod": "NODE_ENV=prod node dist/index.js",
    "build": "npm run compile && npm run path:resolve",
    "compile": "rm -rf dist && tsc",
    "path:resolve": "tsc-alias -p tsconfig.json",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "migrate": "sequelize db:migrate",
    "migrate-undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed-undo": "sequelize db:seed:undo"
  },
  "author": "Folio3",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jose": "^4.14.4",
    "mysql2": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/node": "^20.2.3",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  }
}
